{
  "expo": {
    "name": "Quick Planner",
    "slug": "quick-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/graphics/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/graphics/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a8383b"
    },
    "assetBundlePatterns": [
      "**/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/graphics/adaptive-icon.png",
		"monochromeImage": "./assets/graphics/adaptive-icon.png",
        "backgroundColor": "#fffafb"
      },
      "package": "ir.foreverspring.quickplanner"
    },
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "ec392ba8-0216-444b-bd95-0debcc67573f"
      }
    },
    "plugins": [
      "expo-localization"
    ],
    "owner": "foreverspring"
  }
}
